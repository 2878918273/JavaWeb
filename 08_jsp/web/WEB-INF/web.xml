<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>_1_View</servlet-name>
        <servlet-class>_01_JspView._1_View</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>_1_View</servlet-name>
        <url-pattern>/_1_View1</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>SearchStudentServlet</servlet-name>
        <servlet-class>_02_请求转发说明.SearchStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchStudentServlet</servlet-name>
        <url-pattern>/SearchStudentServlet</url-pattern>
    </servlet-mapping>

    <!--  配置监听器    -->
    <listener>
        <listener-class>_03_Listener.MyServletContextListenerImpl</listener-class>
    </listener>

</web-app>